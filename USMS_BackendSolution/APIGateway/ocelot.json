{
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7067"
  },
  "Routes": [
    /* Room API Start */
    {
      "DownstreamPathTemplate": "/api/Room",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7131
        }
      ],
      "UpstreamPathTemplate": "/api/Room",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT" ]
    },
    {
      "DownstreamPathTemplate": "/api/Room/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7131
        }
      ],
      "UpstreamPathTemplate": "/api/Room/{id}",
      "UpstreamHttpMethod": [ "GET", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/api/Room/ChangeStatus/{id}/{status}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7131
        }
      ],
      "UpstreamPathTemplate": "/api/Room/ChangeStatus/{id}/{status}",
      "UpstreamHttpMethod": [ "GET" ]
    },
    /* Room API End */

    /* Subject API Start */
    {
      "DownstreamPathTemplate": "/api/Subjects",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7286
        }
      ],
      "UpstreamPathTemplate": "/api/Subjects",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT" ]
    },
    {
      "DownstreamPathTemplate": "/api/Subjects/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7286
        }
      ],
      "UpstreamPathTemplate": "/api/Subjects/{id}",
      "UpstreamHttpMethod": [ "GET", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/api/Subjects/ChangeStatus/{id}/{status}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7286
        }
      ],
      "UpstreamPathTemplate": "/api/Subjects/ChangeStatus/{id}/{status}",
      "UpstreamHttpMethod": [ "GET" ]
    }
    /* Subject API End */
  ]
}
